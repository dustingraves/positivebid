<h1>Listing questions</h1>

<table>
  <tr>
    <th>Faq</th>
    <th>Title</th>
    <th>Body</th>
    <th>Published</th>
  </tr>

<% for question in @questions %>
  <tr>
    <td><%= question.faq_id %></td>
    <td><%= question.title %></td>
    <td><%= question.body %></td>
    <td><%= question.published %></td>
    <td><%= link_to 'Show', resource_path(question) %></td>
  </tr>
<% end %>
</table>

<br />

